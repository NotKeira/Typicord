{
  "name": "typicord",
  "version": "3.2.5",
  "description": "A modern, efficient Discord API library for Node.js with full TypeScript support",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "keywords": [
    "typicord",
    "discord",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "NotKeira <node-pkgs@accounts.keira.boo> (https://keirahopkins.co.uk)",
  "repository": {
    "type": "git",
    "url": "https://git.keira.boo/Typicord.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist typicord-*.tgz dist",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "packageManager": "pnpm@10.16.1+sha256.b77e92ba0d59a6372b6c5041bbb3f866fb85e927df333827f0c7f577c5e1a713",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^24.4.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "benchmark": "^2.1.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "globals": "^16.4.0",
    "undici": "^7.16.0",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  }
}
