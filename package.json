{
  "name": "typicord",
  "version": "1.8.2",
  "description": "Typicord is a TypeScript library for interacting with the Discord API, providing type definitions and utilities to simplify development.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "keywords": [
    "typicord",
    "discord",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "NotKeira <node-pkgs@accounts.keira.boo> (https://keirahopkins.co.uk)",
  "repository": {
    "type": "git",
    "url": "https://git.keira.boo/Typicord.git"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist typicord-*.tgz",
    "prepublishOnly": "pnpm run build",
    "test": "npm-run-all --parallel test:unit test:integration",
    "test:unit": "SKIP_INTEGRATION_TESTS=true pnpm tsx ./tests/restclient.test.ts && pnpm tsx ./tests/agent-reuse.test.ts",
    "test:integration": "SKIP_INTEGRATION_TESTS=true pnpm tsx ./tests/reconnection.test.ts",
    "test:bot": "SKIP_INTEGRATION_TESTS=true pnpm tsx ./tests/bot.test.ts",
    "test:ci": "SKIP_INTEGRATION_TESTS=true npm-run-all test:unit test:integration --continue-on-error",
    "lint": "eslint src/ tests/ examples/ benchmarks/",
    "lint:fix": "eslint src/ tests/ examples/ benchmarks/ --fix",
    "format": "prettier --write src/ tests/ examples/ benchmarks/",
    "format:check": "prettier --check src/ tests/ examples/ benchmarks/",
    "quality": "npm-run-all --parallel lint format:check --continue-on-error",
    "quality:fix": "npm-run-all lint:fix format",
    "bench": "npm-run-all bench:rest bench:messages bench:gateway bench:memory --continue-on-error",
    "bench:rest": "pnpm tsx ./benchmarks/rest-client.bench.ts",
    "bench:messages": "pnpm tsx ./benchmarks/message-processing.bench.ts",
    "bench:gateway": "pnpm tsx ./benchmarks/gateway.bench.ts",
    "bench:memory": "pnpm tsx --expose-gc ./benchmarks/memory.bench.ts",
    "ci": "npm-run-all build quality test:ci bench --continue-on-error"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^22.17.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "benchmark": "^2.1.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "globals": "^16.3.0",
    "undici": "^7.13.0",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  }
}