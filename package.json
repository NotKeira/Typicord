{
  "name": "typicord",
  "version": "1.0.3",
  "description": "Typicord is a TypeScript library for interacting with the Discord API, providing type definitions and utilities to simplify development.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "test": "npm-run-all test:*",
    "test:unit": "npm-run-all test:rest test:reconnection",
    "test:integration": "npm-run-all test:bot",
    "test:rest": "tsx ./tests/restclient.test.ts",
    "test:bot": "tsx ./tests/bot.test.ts",
    "test:reconnection": "tsx ./tests/reconnection.test.ts",
    "lint": "eslint src/ tests/ examples/",
    "lint:fix": "eslint src/ tests/ examples/ --fix",
    "format": "prettier --write src/ tests/ examples/",
    "format:check": "prettier --check src/ tests/ examples/",
    "bench": "tsx ./benchmarks/index.ts",
    "bench:rest": "timeout 30s tsx ./benchmarks/rest-client.bench.ts || true",
    "bench:messages": "timeout 30s tsx ./benchmarks/message-processing.bench.ts || true",
    "bench:gateway": "timeout 30s tsx ./benchmarks/gateway.bench.ts || true",
    "bench:memory": "tsx --expose-gc ./benchmarks/memory.bench.ts"
  },
  "keywords": [
    "typicord",
    "discord",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "NotKeira <Keira Hopkins> (https://git.keira.boo)",
  "repository": {
    "type": "git",
    "url": "https://git.keira.boo/Typicord.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^22.15.31",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "benchmark": "^2.1.4",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "dependencies": {
    "dotenv": "^17.0.0",
    "globals": "^16.2.0",
    "undici": "^7.11.0",
    "ws": "^8.18.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  }
}