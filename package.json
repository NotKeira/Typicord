{
  "name": "typicord",
  "version": "3.2.3",
  "description": "A modern, efficient Discord API library for Node.js with full TypeScript support",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "keywords": [
    "typicord",
    "discord",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "NotKeira <node-pkgs@accounts.keira.boo> (https://keirahopkins.co.uk)",
  "repository": {
    "type": "git",
    "url": "https://git.keira.boo/Typicord.git"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist typicord-*.tgz dist",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^22.17.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "benchmark": "^2.1.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "globals": "^16.3.0",
    "undici": "^7.13.0",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  }
}